<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListarUsers</servlet-name>
        <servlet-class>com.dws.servlets.ListarUsers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarCanciones</servlet-name>
        <servlet-class>com.dws.servlets.ListarCanciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearCancion</servlet-name>
        <servlet-class>com.dws.servlets.CrearCancion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarCancion</servlet-name>
        <servlet-class>com.dws.servlets.ModificarCancion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarCancion</servlet-name>
        <servlet-class>com.dws.servlets.EliminarCancion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminaCancion</servlet-name>
        <servlet-class>com.dws.servlets.EliminaCancion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListarUsers</servlet-name>
        <url-pattern>/ListarUsers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarCanciones</servlet-name>
        <url-pattern>/ListarCanciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearCancion</servlet-name>
        <url-pattern>/CrearCancion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarCancion</servlet-name>
        <url-pattern>/ModificarCancion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminaCancion</servlet-name>
        <url-pattern>/EliminaCancion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
